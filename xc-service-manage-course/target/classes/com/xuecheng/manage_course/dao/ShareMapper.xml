<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xuecheng.manage_course.dao.ShareMapper">


    <insert id="insertShareBean" useGeneratedKeys="true" keyProperty="id" keyColumn="id"
            parameterType="com.xuecheng.manage_course.entity.Share">

    INSERT INTO tab_share(
    id,
    share_id,
    share_header,
    share_name,
    score
    )
    VALUES(
        null,
       #{share_id},
       #{share_header},
       #{share_name},
       #{score}
      )

    </insert>


</mapper>